<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>yoke-pilot.ac</path>
	
	<model>
		<name>Cable</name>
		<path>cable.xml</path>
		<offsets>
			<z-m>-0.05</z-m>
		</offsets>
	</model>
	
	<animation>
		<name>Controls</name>
		<object-name>AutopilotDisengageSwitch</object-name>
		<object-name>AutopilotDisengageSwitchBar</object-name>
		<object-name>AutopilotDisengageSwitchAxis</object-name>
		<object-name>TrimDisengageSwitch</object-name>
		<object-name>TrimDisengageSwitchAxis</object-name>
		<object-name>TrimDisengageSwitchBar</object-name>
		<object-name>GoAroundButton</object-name>
		<object-name>GoAroundButtonAxis</object-name>
		<object-name>PitchSyncButton</object-name>
		<object-name>PitchSyncButtonAxis</object-name>
		<object-name>PitchTrimSwitch</object-name>
		<object-name>PitchTrimSwitchAxis</object-name>
		<object-name>IdentButton</object-name>
		<object-name>IdentButtonAxis</object-name>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>Bar</object-name>
		<object-name>Middle</object-name>
		<object-name>Handle</object-name>
		<object-name>Controls</object-name>
		<object-name>ButtonsMount</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>controls/flight/elevator</property>
		<factor>0.125</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Middle</object-name>
		<object-name>Handle</object-name>
		<object-name>Bar</object-name>
		<object-name>Controls</object-name>
		<object-name>ButtonsMount</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>controls/flight/aileron</property>
		<factor>60</factor>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>Mount</object-name>
		<object-name>Bar</object-name>
		<action>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/controls/show-yokes</property>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>yokes-show</tooltip-id>
				<label>Show / hide yokes</label>
			</binding>
		</hovered>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Bar</object-name>
		<object-name>Middle</object-name>
		<object-name>Handle</object-name>
		<object-name>Controls</object-name>
		<object-name>ButtonsMount</object-name>
		<object-name>Cable</object-name>
		<condition>
			<property>sim/model/controls/show-yokes</property>
		</condition>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>AutopilotDisengageSwitch</object-name>
		<object-name>AutopilotDisengageSwitchBar</object-name>
		<axis>
			<object-name>AutopilotDisengageSwitchAxis</object-name>
		</axis>
		<property>autopilot/if-550a/disengage-pressed</property>
		<factor>20</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/disengage-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>if550a.disengage();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/disengage-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
		<hovered>
			<command>set-tooltip</command>
			<tooltip-id>yoke-autopilot-disengage</tooltip-id>
			<label>Disengage autopilot</label>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>TrimDisengageSwitch</object-name>
		<object-name>TrimDisengageSwitchBar</object-name>
		<axis>
			<object-name>TrimDisengageSwitchAxis</object-name>
		</axis>
		<property>controls/flight/electric-trim-engaged</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>20</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<action>
			<binding>
				<command>property-toggle</command>
				<property>controls/flight/electric-trim-engaged</property>
			</binding>
		</action>
		<hovered>
			<command>set-tooltip</command>
			<tooltip-id>yoke-electric-trim-disengage</tooltip-id>
			<label>Electric elevator trim: %s</label>
			<measure-text>Electric elevator trim: Disengaged</measure-text>
			<property>controls/flight/electric-trim-engaged</property>
			<mapping>nasal</mapping>
			<script>
				return ["Disengaged", "Engaged"][arg[0]];
			</script>
		</hovered>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>PitchTrimSwitch</object-name>
		<increase>
			<binding>
				<command>property-assign</command>
				<property>controls/flight/electric-trim-actuate</property>
				<value>1</value>
			</binding>
		</increase>
		<decrease>
			<binding>
				<command>property-assign</command>
				<property>controls/flight/electric-trim-actuate</property>
				<value>-1</value>
			</binding>
		</decrease>
		<release>
			<binding>
				<command>property-assign</command>
				<property>controls/flight/electric-trim-actuate</property>
				<value>0</value>
			</binding>
		</release>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>yoke-electric-trim-actuate</tooltip-id>
				<label>Elevator trim: %2.1f°</label>
				<measure-text>Elevator trim: 22.9°</measure-text>
				<property>controls/flight/elevator-trim</property>
				<mapping>nasal</mapping>
				<script><![CDATA[
					var value = arg[0];
					if (value < 1) {
						return value * -17;
					} else {
						return value * -23;
					}
				]]></script>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>PitchTrimSwitch</object-name>
		<axis>
			<object-name>PitchTrimSwitchAxis</object-name>
		</axis>
		<property>controls/flight/electric-trim-actuate</property>
		<factor>0.4</factor>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>GoAroundButton</object-name>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/go-around-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>if550a.toggleGoAroundMode();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/go-around-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>yoke-go-around</tooltip-id>
				<label>AP go-around mode: %s</label>
				<measure-text>AP go-around mode: Inactive</measure-text>
				<property>autopilot/if-550a/modes/go-around</property>
				<mapping>nasal</mapping>
				<script>
					return ["Inactive", "Active"][arg[0]];
				</script>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>GoAroundButton</object-name>
		<axis>
			<object-name>GoAroundButtonAxis</object-name>
		</axis>
		<property>autopilot/if-550a/go-around-pressed</property>
		<factor>0.25</factor>
	</animation>

	<animation>
		<type>slider</type>
		<object-name>PitchSyncButton</object-name>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/pitch-sync-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/target-pitch-deg</property>
				<property>instrumentation/attitude-indicator/indicated-pitch-deg</property>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/pitch-sync-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>yoke-pitch-sync</tooltip-id>
				<label>Set flight director target pitch to current pitch</label>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>PitchSyncButton</object-name>
		<axis>
			<object-name>PitchSyncButtonAxis</object-name>
		</axis>
		<property>autopilot/if-550a/pitch-sync-pressed</property>
		<factor>0.25</factor>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>IdentButton</object-name>
		<action>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/transponder/inputs/yoke-ident-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/transponder/inputs/ident-btn</property>
				<value type="bool">true</value>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>instrumentation/transponder/inputs/yoke-ident-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/transponder/inputs/ident-btn</property>
				<value type="bool">false</value>
			</binding>
		</release>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>transponder-ident</tooltip-id>
				<label>Transponder Ident</label>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>IdentButton</object-name>
		<axis>
			<object-name>IdentButtonAxis</object-name>
		</axis>
		<property>instrumentation/transponder/inputs/yoke-ident-pressed</property>
		<factor>-0.25</factor>
	</animation>
	
	<effect>
		<inherits-from>../../../../Effects/p210n-interior-metal</inherits-from>
		<object-name>AutopilotDisengageSwitchBar</object-name>
		<object-name>TrimDisengageSwitchBar</object-name>
	</effect>
	<effect>
		<inherits-from>yoke-normalmap</inherits-from>
		<object-name>Middle</object-name>
	</effect>
</PropertyList>
