<?xml version="1.0"?>
<PropertyList>
	<path>right-controls-panel.ac</path>
	
	<model>
		<path>flaps/flaps.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>-0.1</y-m>
			<z-m>0</z-m>
		</offsets>
	</model>
	
	<model>
		<path>valve/valve.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>-0.05</y-m>
			<z-m>-0.04</z-m>
		</offsets>
		<overlay>
			<params>
				<property>controls/cabin-air/cabin-heat</property>
				<tooltip-id>cabin-heat</tooltip-id>
				<label>Cabin heat: %s</label>
				<measure-text>Cabin heat: Off</measure-text>
			</params>
		</overlay>
	</model>
	
	<model>
		<path>valve/valve.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>-0.05</y-m>
			<z-m>0.03</z-m>
		</offsets>
		<overlay>
			<params>
				<property>controls/cabin-air/defrost</property>
				<tooltip-id>cabin-defrost</tooltip-id>
				<label>Cabin defrost: %s</label>
				<measure-text>Cabin defrost: Off</measure-text>
			</params>
		</overlay>
	</model>
	
	<model>
		<path>switches/overhead-vent-fan.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.04</z-m>
		</offsets>
	</model>
	
	<model>
		<path>valve/valve.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0.05</y-m>
			<z-m>-0.04</z-m>
		</offsets>
		<overlay>
			<params>
				<property>controls/cabin-air/floor-defrost</property>
				<tooltip-id>floor-defrost</tooltip-id>
				<label>Floor defrost: %s</label>
				<measure-text>Floor defrost: Off</measure-text>
			</params>
		</overlay>
	</model>
</PropertyList>
