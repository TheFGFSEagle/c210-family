<?xml version="1.0"?>
<PropertyList>
	<path>cabin-alt.ac</path>
	
	<animation>
		<type>rotate</type>
		<object-name>AltNeedle</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/pressurization/indicated-cabin-altitude-ft</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>5000</ind>
				<dep>70</dep>
			</entry>
			<entry>
				<ind>10000</ind>
				<dep>130</dep>
			</entry>
			<entry>
				<ind>15000</ind>
				<dep>180</dep>
			</entry>
			<entry>
				<ind>20000</ind>
				<dep>220</dep>
			</entry>
			<entry>
				<ind>25000</ind>
				<dep>255</dep>
			</entry>
			<entry>
				<ind>30000</ind>
				<dep>285</dep>
			</entry>
			<entry>
				<ind>35000</ind>
				<dep>310</dep>
			</entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>PSINeedle</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/pressurization/indicated-differential-pressure-psi</property>
		<factor>30</factor>
		<min>0</min>
		<max>6</max>
	</animation>
	
	<text>
		<name>Label</name>
		<type type="string">literal</type>
		<text type="string">Cabin altitude</text>
		<draw-text type="bool">true</draw-text>
		<font>Helvetica.txf</font>
		<character-size type="double">0.01</character-size>
		<character-aspect-ratio type="double">1.0</character-aspect-ratio>
		<axis-alignment>yz-plane</axis-alignment>
		<max-height>0.04</max-height>
		<max-width>0.04</max-width>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
		<alignment>center-center</alignment>
		<offsets>
			<x-m>0.01</x-m>
			<y-m>0.0 </y-m>
			<z-m>0.0</z-m>
		</offsets>
	</text>
	
	<animation>
		<type>material</type>
		<object-name>Label</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Label</object-name>
		<condition>
			<property>/sim/panel-hotspots</property>
		</condition>
	</animation>
	
	<effect>
		<inherits-from>../../../../Effects/p210n-instrument-glass</inherits-from>
		<object-name>Glass</object-name>
	</effect>
</PropertyList>
