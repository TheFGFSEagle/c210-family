<?xml version="1.0"?>
<PropertyList>
	<path>dme.ac</path>
	
	<animation>
		<type>knob</type>
		<object-name>SourceKnob</object-name>
		<axis>
			<object-name>SourceKnobAxis</object-name>
		</axis>
		<property>instrumentation/dme/source-knob</property>
		<factor>20</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/dme/source-knob</property>
				<min>0</min>
				<max>3</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.dme.sourceChanged();</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>ModeSwitch</object-name>
		<axis>
			<object-name>ModeSwitchAxis</object-name>
		</axis>
		<property>instrumentation/dme/mode</property>
		<factor>-20</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/dme/mode</property>
				<min>0</min>
				<max>2</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.dme.modeChanged();</script>
			</binding>
		</action>
		<release>
			<binding>
				<condition>
					<equals>
						<property>instrumentation/dme/mode</property>
						<value>2</value>
					</equals>
				</condition>
				<command>property-interpolate</command>
				<property>instrumentation/dme/mode</property>
				<value>1</value>
				<time>0.01</time>
			</binding>
			<binding>
				<condition>
					<equals>
						<property>instrumentation/dme/mode</property>
						<value>2</value>
					</equals>
				</condition>
				<command>nasal</command>
				<script>c210.dme.modeChanged();</script>
			</binding>
		</release>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>DisplayModeSwitch</object-name>
		<axis>
			<object-name>DisplayModeSwitchAxis</object-name>
		</axis>
		<property>instrumentation/dme/display-mode</property>
		<factor>-40</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/dme/display-mode</property>
				<min>0</min>
				<max>2</max>
				<factor>1</factor>
				<wrap type="bool">true</wrap>
			</binding>
		</action>
	</animation>
	
	<animation>
		<condition>
			<property>instrumentation/dme/powered</property>
		</condition>
		<type>material</type>
		<object-name>DMEDistanceDisplay</object-name>
		<object-name>DMESpeedTimeDisplay</object-name>
		<emission>
			<red>1</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>controls/lighting/panel-integral-norm</factor-prop>
		</emission>
	</animation>
	
	<text>
		<name>Label</name>
		<type type="string">literal</type>
		<text type="string">DME indicator</text>
		<draw-text type="bool">true</draw-text>
		<font>Helvetica.txf</font>
		<character-size type="double">0.015</character-size>
		<character-aspect-ratio type="double">1.0</character-aspect-ratio>
		<axis-alignment>yz-plane</axis-alignment>
		<max-height>0.08</max-height>
		<max-width>0.08</max-width>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
		<alignment>center-center</alignment>
		<offsets>
			<x-m>0.01</x-m>
			<y-m>0.0 </y-m>
			<z-m>0.0</z-m>
		</offsets>
	</text>
	
	<animation>
		<type>material</type>
		<object-name>Label</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Label</object-name>
		<condition>
			<property>/sim/panel-hotspots</property>
		</condition>
	</animation>
	
	<effect>
		<inherits-from>../../../../Effects/p210n-instrument-glass</inherits-from>
		<object-name>Glass</object-name>
	</effect>
</PropertyList>
