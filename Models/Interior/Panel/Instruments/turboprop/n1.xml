<?xml version="1.0"?>
<PropertyList>
	<path>n1.ac</path>
	
	<animation>
		<type>rotate</type>
		<object-name>BigNeedle</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/engine/indicated-n1-percent</property>
		<factor>2.7</factor>
		<min-deg>0</min-deg>
		<max-deg>270</max-deg>
	</animation>
	
	<animation>
		<type>rotate</type>
		<axis>
			<object-name>SmallNeedleAxis</object-name>
		</axis>
		<property>instrumentation/engine/indicated-n1-percent</property>
		<factor>36</factor>
		<min-deg>0</min-deg>
		<max-deg>3960</max-deg>
	</animation>
	
	<text>
		<name>Label</name>
		<type type="string">literal</type>
		<text type="string">Percent N1</text>
		<draw-text type="bool">true</draw-text>
		<font>Helvetica.txf</font>
		<character-size type="double">0.015</character-size>
		<character-aspect-ratio type="double">1.0</character-aspect-ratio>
		<axis-alignment>yz-plane</axis-alignment>
		<max-height>0.05</max-height>
		<max-width>0.05</max-width>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
		<alignment>center-center</alignment>
		<offsets>
			<x-m>0.01</x-m>
			<y-m>0.0</y-m>
			<z-m>0.01</z-m>
		</offsets>
	</text>
	
	<animation>
		<type>material</type>
		<object-name>Label</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Label</object-name>
		<condition>
			<property>/sim/panel-hotspots</property>
		</condition>
	</animation>
	
	<effect>
		<inherits-from>Models/Effects/p210n-instrument-glass</inherits-from>
		<object-name>Glass</object-name>
	</effect>
</PropertyList>
