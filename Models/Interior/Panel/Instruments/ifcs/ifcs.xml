<?xml version="1.0"?>
<PropertyList>
	<path>ifcs.ac</path>
	
	<animation>
		<type>slider</type>
		<object-name>FDButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/ifcs/fd-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/ifcs/fd-pressed</property>
				<value>1</value>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>instrumentation/ifcs/fd-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
