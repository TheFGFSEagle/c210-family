<?xml version="1.0"?>
<PropertyList>
	<path>aa-801a.ac</path>
	
	<animation>
		<type>slider</type>
		<object-name>ArmButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/alt-alert/arm-button-pressed</property>
		<factor>0.002</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/alt-alert/arm-button-pressed</property>
				<value>1</value>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>instrumentation/alt-alert/arm-button-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>Minilever100</object-name>
		<axis>
			<object-name>MinileversScalesAxis</object-name>
		</axis>
		<property>instrumentation/alt-alert/selected-altitude-ft-hundreds</property>
		<factor>-7.4</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/alt-alert/selected-altitude-ft-hundreds</property>
				<factor>1</factor>
				<min>0</min>
				<max>9</max>
				<wrap>false</wrap>
			</binding>
			<binding>
				<command>nasal</command>
				<script>aa801a.updateSelectedAltitude();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>alt-alert-altitude</tooltip-id>
				<label>Selected alert altitude: %d ft</label>
				<measure-text>Selected alert altitude: 23000 ft</measure-text>
				<property>instrumentation/alt-alert/selected-altitude-ft</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>Minilever1000</object-name>
		<axis>
			<object-name>MinileversScalesAxis</object-name>
		</axis>
		<property>instrumentation/alt-alert/selected-altitude-ft-thousands</property>
		<factor>-7.4</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/alt-alert/selected-altitude-ft-thousands</property>
				<factor>1</factor>
				<min>0</min>
				<max>9</max>
				<wrap>false</wrap>
			</binding>
			<binding>
				<command>nasal</command>
				<script>aa801a.updateSelectedAltitude();</script>
			</binding>	
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>alt-alert-altitude</tooltip-id>
				<label>Selected alert altitude: %d ft</label>
				<measure-text>Selected alert altitude: 23000 ft</measure-text>
				<property>instrumentation/alt-alert/selected-altitude-ft</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>Minilever10000</object-name>
		<axis>
			<object-name>MinileversScalesAxis</object-name>
		</axis>
		<property>instrumentation/alt-alert/selected-altitude-ft-tenthousands</property>
		<factor>-7.4</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/alt-alert/selected-altitude-ft-tenthousands</property>
				<factor>1</factor>
				<min>0</min>
				<max>2</max>
				<wrap>false</wrap>
			</binding>
			<binding>
				<command>nasal</command>
				<script>aa801a.updateSelectedAltitude();</script>
			</binding>	
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>alt-alert-altitude</tooltip-id>
				<label>Selected alert altitude: %d ft</label>
				<measure-text>Selected alert altitude: 23000 ft</measure-text>
				<property>instrumentation/alt-alert/selected-altitude-ft</property>
			</binding>
		</hovered>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>AltScale100</object-name>
		<axis>
			<object-name>MinileversScalesAxis</object-name>
		</axis>
		<property>instrumentation/alt-alert/selected-altitude-ft-hundreds</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>AltScale1000</object-name>
		<axis>
			<object-name>MinileversScalesAxis</object-name>
		</axis>
		<property>instrumentation/alt-alert/selected-altitude-ft-thousands</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>AltScale10000</object-name>
		<object-name>Flag</object-name>
		<axis>
			<object-name>MinileversScalesAxis</object-name>
		</axis>
		<property>instrumentation/alt-alert/selected-altitude-ft-tenthousands</property>
		<factor>-36</factor>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>HighLamp</object-name>
		<emission>
			<red>1</red>
			<green>0.5</green>
			<blue>0.25</blue>
			<factor-prop>instrumentation/alt-alert/high-alert</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>material</type>
		<object-name>LevelLamp</object-name>
		<emission>
			<red>0.2</red>
			<green>0.6</green>
			<blue>0.2</blue>
			<factor-prop>instrumentation/alt-alert/level-alert</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>material</type>
		<object-name>LowLamp</object-name>
		<emission>
			<red>1</red>
			<green>0.5</green>
			<blue>0.25</blue>
			<factor-prop>instrumentation/alt-alert/low-alert</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>material</type>
		<object-name>ArmedLamp</object-name>
		<emission>
			<red>1</red>
			<green>0.5</green>
			<blue>0.25</blue>
			<factor-prop>instrumentation/alt-alert/armed</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>material</type>
		<object-name>CoupledLamp</object-name>
		<emission>
			<red>0.2</red>
			<green>0.6</green>
			<blue>0.2</blue>
			<factor-prop>instrumentation/alt-alert/coupled</factor-prop>
		</emission>
	</animation>
	
	 <text>
		<name>Label</name>
		<type type="string">literal</type>
		<text type="string">Altitude alerter</text>
		<draw-text type="bool">true</draw-text>
		<font>Helvetica.txf</font>
		<character-size type="double">0.015</character-size>
		<character-aspect-ratio type="double">1.0</character-aspect-ratio>
		<axis-alignment>yz-plane</axis-alignment>
		<max-height>0.04</max-height>
		<max-width>0.08</max-width>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
		<alignment>center-center</alignment>
		<offsets>
			<x-m>0.01</x-m>
			<y-m>0.0 </y-m>
			<z-m>0.0</z-m>
		</offsets>
	</text>
	
	<animation>
		<type>material</type>
		<object-name>Label</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Label</object-name>
		<condition>
			<property>/sim/panel-hotspots</property>
		</condition>
	</animation>
	
	<effect>
		<inherits-from>../../../../Effects/p210n-interior-metal</inherits-from>
		<object-name>Minilever100</object-name>
		<object-name>Minilever1000</object-name>
		<object-name>Minilever10000</object-name>
	</effect>
</PropertyList>
