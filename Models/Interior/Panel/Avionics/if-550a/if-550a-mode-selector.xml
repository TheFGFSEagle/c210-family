<?xml version="1.0"?>
<PropertyList>
	<path>if-550a-mode-selector.ac</path>
	
	<animation>
		<type>slider</type>
		<object-name>FDButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>autopilot/if-550a/fd-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/fd-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>if550a.toggleFlightDirector();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/fd-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>Nav1Button</object-name>
		<object-name>Nav2Button</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>autopilot/if-550a/nav-source-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/nav-source-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>if550a.toggleNavSource();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/nav-source-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>HDGButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>autopilot/if-550a/hdg-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/hdg-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>if550a.toggleHeadingMode();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/hdg-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	<animation>
		<type>slider</type>
		<object-name>NavButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>autopilot/if-550a/nav-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/nav-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>if550a.toggleNavMode();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/nav-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	<animation>
		<type>slider</type>
		<object-name>BackCourseButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>autopilot/if-550a/backcourse-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/backcourse-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>if550a.toggleBackcourseMode();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/backcourse-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>AltButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>autopilot/if-550a/alt-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>autopilot/if-550a/alt-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>if550a.toggleAltMode();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>autopilot/if-550a/alt-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
	<animation>
		<condition>
			<property>autopilot/powered</property>
		</condition>
		<type>material</type>
		<object-name>APAnnunciator</object-name>
		<object-name>FDButton</object-name>
		<object-name>Nav1Button</object-name>
		<object-name>Nav2Button</object-name>
		<object-name>HDGButton</object-name>
		<object-name>NavButton</object-name>
		<object-name>BackCourseButton</object-name>
		<object-name>AltButton</object-name>
		<object-name>VORAnnunciator</object-name>
		<object-name>LOCAnnunciator</object-name>
		<object-name>GSAnnunciator</object-name>
		<object-name>GoAroundAnnunciator</object-name>
		<emission>
			<red>0</red>
			<green>1</green>
			<blue>0</blue>
			<factor-prop>autopilot/if-550a/green-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<property>autopilot/if-550a/engaged</property>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>APAnnunciator</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<property>autopilot/if-550a/flight-director</property>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>FDButton</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<equals>
							<property>autopilot/if-550a/internal/nav-source</property>
							<value>0</value>
						</equals>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>Nav1Button</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<equals>
							<property>autopilot/if-550a/internal/nav-source</property>
							<value>1</value>
						</equals>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>Nav2Button</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<property>autopilot/if-550a/modes/heading</property>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>HDGButton</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<property>autopilot/if-550a/modes/nav</property>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>NavButton</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<property>autopilot/if-550a/modes/backcourse</property>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>BackCourseButton</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<property>autopilot/if-550a/modes/altitude</property>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>AltButton</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<equals>
							<property>autopilot/if-550a/internal/nav-type</property>
							<value>vor</value>
						</equals>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>VORAnnunciator</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<equals>
							<property>autopilot/if-550a/internal/nav-type</property>
							<value>localizer</value>
						</equals>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>LOCAnnunciator</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<equals>
							<property>autopilot/if-550a/internal/gs-captured</property>
							<value>vor</value>
						</equals>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>GSAnnunciator</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<condition>
			<and>
				<not>
					<or>
						<property>autopilot/if-550a/green-light-test</property>
						<property>autopilot/if-550a/modes/go-around</property>
					</or>
				</not>
				<property>autopilot/powered</property>
			</and>
		</condition>
		<type>material</type>
		<object-name>GoAroundAnnunciator</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>autopilot/if-550a/white-light-intensity-norm</factor-prop>
		</emission>
	</animation>
	
	<text>
		<name>Label</name>
		<type type="string">literal</type>
		<text type="string">Autopilot mode selector and annunciator</text>
		<draw-text type="bool">true</draw-text>
		<font>Helvetica.txf</font>
		<character-size type="double">0.015</character-size>
		<character-aspect-ratio type="double">1.0</character-aspect-ratio>
		<axis-alignment>yz-plane</axis-alignment>
		<max-height>0.08</max-height>
		<max-width>0.08</max-width>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
		<alignment>center-center</alignment>
		<offsets>
			<x-m>0.01</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</offsets>
	</text>
	
	<animation>
		<type>material</type>
		<object-name>Label</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Label</object-name>
		<condition>
			<property>/sim/panel-hotspots</property>
		</condition>
	</animation>
</PropertyList>
