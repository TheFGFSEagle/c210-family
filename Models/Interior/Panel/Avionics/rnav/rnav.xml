			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-bearing-deg-units</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>p210n.rnav.updateBearing();</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>BearingMinilever0.1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-decimals</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-bearing-deg-decimals</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
