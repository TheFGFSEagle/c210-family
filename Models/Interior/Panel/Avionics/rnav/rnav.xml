			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>DistanceMinilever100</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-hundreds</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-distance-nm-hundreds</property>
				<min>0</min>
				<max>2</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>p210n.rnav.updateDistance();</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>DistanceMinilever10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-tenths</property>
		<factor>-7</factor>
		<action>
