<?xml version="1.0"?>
<PropertyList>
	<path>rnav.ac</path>
	
	<animation>
		<type>knob</type>
		<object-name>BearingMinilever100</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-hundreds</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-bearing-deg-hundreds</property>
				<min>0</min>
				<max>3</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateBearing();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-bearing</tooltip-id>
				<label>Waypoint bearing: %3.1f°</label>
				<measure-text>Waypoint bearing: 359.9°</measure-text>
				<property>instrumentation/rnav/selected-bearing-deg</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>BearingMinilever10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-tenths</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-bearing-deg-tenths</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateBearing();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-bearing</tooltip-id>
				<label>Waypoint bearing: %3.1f°</label>
				<measure-text>Waypoint bearing: 359.9°</measure-text>
				<property>instrumentation/rnav/selected-bearing-deg</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>BearingMinilever1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-units</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-bearing-deg-units</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateBearing();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-bearing</tooltip-id>
				<label>Waypoint bearing: %3.1f°</label>
				<measure-text>Waypoint bearing: 359.9°</measure-text>
				<property>instrumentation/rnav/selected-bearing-deg</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>BearingMinilever0.1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-decimals</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-bearing-deg-decimals</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateBearing();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-bearing</tooltip-id>
				<label>Waypoint bearing: %3.1f°</label>
				<measure-text>Waypoint bearing: 359.9°</measure-text>
				<property>instrumentation/rnav/selected-bearing-deg</property>
			</binding>
		</hovered>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>DistanceMinilever100</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-hundreds</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-distance-nm-hundreds</property>
				<min>0</min>
				<max>2</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateDistance();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-distance</tooltip-id>
				<label>Waypoint distance: %3.1f°</label>
				<measure-text>Waypoint distance: 200.0°</measure-text>
				<property>instrumentation/rnav/selected-distance-nm</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>DistanceMinilever10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-tenths</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-distance-nm-tenths</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateDistance();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-distance</tooltip-id>
				<label>Waypoint distance: %3.1f°</label>
				<measure-text>Waypoint distance: 200.0°</measure-text>
				<property>instrumentation/rnav/selected-distance-nm</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>DistanceMinilever1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-units</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-distance-nm-units</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateDistance();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-distance</tooltip-id>
				<label>Waypoint distance: %3.1f°</label>
				<measure-text>Waypoint distance: 200.0°</measure-text>
				<property>instrumentation/rnav/selected-distance-nm</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>DistanceMinilever0.1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-decimals</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-distance-nm-decimals</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateDistance();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-distance</tooltip-id>
				<label>Waypoint distance: %3.1f°</label>
				<measure-text>Waypoint distance: 200.0°</measure-text>
				<property>instrumentation/rnav/selected-distance-nm</property>
			</binding>
		</hovered>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>BearingScale100</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-hundreds</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>BearingScale10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-tenths</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>BearingScale1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-units</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>BearingScale0.1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-decimals</property>
		<factor>-36</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>DistanceScale100</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-hundreds</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>DistanceScale10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-tenths</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>DistanceScale1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-units</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>DistanceScale0.1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-decimals</property>
		<factor>-36</factor>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>DisplaySwitch</object-name>
		<axis>
			<object-name>DisplayFlySwitchAxis</object-name>
		</axis>
		<property>instrumentation/rnav/display-waypoint</property>
		<offset-deg>-40</offset-deg>
		<factor>40</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/display-waypoint</property>
				<min>1</min>
				<max>3</max> <!-- real max + factor needed with wrap = true-->
				<factor>1</factor>
				<wrap type="bool">true</wrap>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-display-waypoint</tooltip-id>
				<label>Displayed waypoint: %d</label>
				<measure-text>Displayed waypoint: 1</measure-text>
				<property>instrumentation/rnav/display-waypoint</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>FlySwitch</object-name>
		<axis>
			<object-name>DisplayFlySwitchAxis</object-name>
		</axis>
		<property>instrumentation/rnav/fly-waypoint</property>
		<offset-deg>-40</offset-deg>
		<factor>40</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/fly-waypoint</property>
				<min>1</min>
				<max>3</max> <!-- real max + factor needed with wrap = true-->
				<factor>1</factor>
				<wrap type="bool">true</wrap>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.updateCurrentWaypoint();</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-fly-waypoint</tooltip-id>
				<label>Fly waypoint: %d</label>
				<measure-text>Fly waypoint: 1</measure-text>
				<property>instrumentation/rnav/fly-waypoint</property>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>ModeSwitch</object-name>
		<axis>
			<object-name>ModeSwitchAxis</object-name>
		</axis>
		<property>instrumentation/rnav/mode</property>
		<factor>40</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/mode</property>
				<min>0</min>
				<max>2</max> <!-- real max + factor needed with wrap = true-->
				<factor>1</factor>
				<wrap type="bool">true</wrap>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-mode</tooltip-id>
				<label>Mode: %s</label>
				<measure-text>Mode: Enroute</measure-text>
				<property>instrumentation/rnav/mode</property>
				<mapping>nasal</mapping>
				<script>
					return ["Enroute", "Approach"][arg[0]];
				</script>
			</binding>
		</hovered>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>TransferButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/rnav/transfer-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/rnav/transfer-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>c210.rnav.transferPressed();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>instrumentation/rnav/transfer-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>rnav-display-waypoint</tooltip-id>
				<label>Push to transfer selected bearing and distance into waypoint 1 or 2 as selected by the Display switch</label>
			</binding>
		</hovered>
	</animation>
	
	<animation>
		<condition>
			<property>instrumentation/rnav/powered</property>
		</condition>
		<type>material</type>
		<object-name>RNAVBearingDisplay</object-name>
		<object-name>RNAVDistanceDisplay</object-name>
		<emission>
			<red>1</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>controls/lighting/panel-integral-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>DisplayFlyMismatchLamp</object-name>
		<condition>
			<and>
				<not>
					<equals>
						<property>instrumentation/rnav/display-waypoint</property>
						<property>instrumentation/rnav/fly-waypoint</property>
					</equals>
				</not>
				<property>instrumentation/rnav/powered</property>
			</and>
		</condition>
		<emission>
			<red>0.6</red>
			<green>0.2</green>
			<blue>0</blue>
		</emission>
	</animation>
	
	<effect>
		<inherits-from>../../../../Effects/p210n-instrument-glass</inherits-from>
		<object-name>Glass</object-name>
	</effect>
	
	<text>
		<name>Label</name>
		<type type="string">literal</type>
		<text type="string">RNAV control</text>
		<draw-text type="bool">true</draw-text>
		<font>Helvetica.txf</font>
		<character-size type="double">0.015</character-size>
		<character-aspect-ratio type="double">1.0</character-aspect-ratio>
		<axis-alignment>yz-plane</axis-alignment>
		<max-height>0.04</max-height>
		<max-width>0.16</max-width>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
		<alignment>center-center</alignment>
		<offsets>
			<x-m>0.01</x-m>
			<y-m>0</y-m>
			<z-m>0.025</z-m>
		</offsets>
	</text>
	
	<animation>
		<type>material</type>
		<object-name>Label</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Label</object-name>
		<condition>
			<property>/sim/panel-hotspots</property>
		</condition>
	</animation>
</PropertyList>
