<?xml version="1.0"?>
<PropertyList>
	<path>rnav.ac</path>
	
	<animation>
		<type>knob</type>
		<object-name>BearingMinilever100</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-hundreds</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-bearing-deg-hundreds</property>
				<min>0</min>
				<max>3</max>
				<factor>1</factor>
			<binding>
				<command>nasal</command>
				<script>p210n.rnav.updateBearing();</script>
			</binding>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>BearingMinilever10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-tenths</property>
		<factor>-7</factor>
		<action>
