	<animation>
		<type>rotate</type>
		<object-name>BearingScale100</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-hundreds</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>BearingScale10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-tenths</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>BearingScale1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-units</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>BearingScale0.1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-bearing-deg-decimals</property>
		<factor>-36</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>DistanceScale100</object-name>
		<axis>
