	<animation>
		<type>knob</type>
		<object-name>DistanceMinilever0.1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-decimals</property>
		<factor>-7</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-distance-nm-decimals</property>
				<min>0</min>
