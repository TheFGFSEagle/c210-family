		</axis>
		<property>instrumentation/rnav/selected-distance-nm-hundreds</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>DistanceScale10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-tenths</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>DistanceScale1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-units</property>
		<factor>-36</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>DistanceScale0.1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property>instrumentation/rnav/selected-distance-nm-decimals</property>
		<factor>-36</factor>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>DisplaySwitch</object-name>
		<axis>
			<object-name>DisplayFlySwitchAxis</object-name>
		</axis>
		<property>instrumentation/rnav/display-waypoint</property>
		<offset>-1</offset>
		<factor>40</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/display-waypoint</property>
				<min>1</min>
				<max>3</max> <!-- real max + factor needed with wrap = true-->
				<factor>1</factor>
				<wrap type="bool">true</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>FlySwitch</object-name>
		<axis>
			<object-name>DisplayFlySwitchAxis</object-name>
		</axis>
		<property>instrumentation/rnav/fly-waypoint</property>
		<offset>-1</offset>
		<factor>40</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/fly-waypoint</property>
				<min>1</min>
				<max>3</max> <!-- real max + factor needed with wrap = true-->
				<factor>1</factor>
				<wrap type="bool">true</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>SensitivitySwitch</object-name>
		<axis>
			<object-name>SensitivitySwitchAxis</object-name>
		</axis>
		<property>instrumentation/rnav/sensitivity</property>
		<offset>-1</offset>
		<factor>40</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/rnav/sensitivity</property>
				<min>0</min>
				<max>2</max> <!-- real max + factor needed with wrap = true-->
				<factor>1</factor>
				<wrap type="bool">true</wrap>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>TransferButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/rnav/transfer-pressed</property>
		<factor>0.004</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/rnav/transfer-pressed</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>p210n.rnav.transferPressed();</script>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property>instrumentation/rnav/transfer-pressed</property>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>DisplayFlyMismatchLamp</object-name>
		<condition>
			<not>
				<equals>
					<property>instrumentation/rnav/display-waypoint</property>
					<property>instrumentation/rnav/fly-waypoint</property>
				</equals>
			</not>
		</condition>
		<emission>
			<red>0.6</red>
			<green>0.2</green>
			<blue>0</blue>
		</emission>
	</animation>
</PropertyList>
