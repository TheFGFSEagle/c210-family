				<command>property-adjust</command>
				<property>instrumentation/rnav/selected-bearing-deg-tenths</property>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>p210n.rnav.updateBearing();</script>
			</binding>
