<?xml version="1.0"?>
<PropertyList>
	<path>adf.ac</path>
	
	<params>
		<frequencies>
			<selected>instrumentation/adf/frequencies/selected</selected>
			<primary>instrumentation/adf/frequencies/primary-khz</primary>
			<primary-thousands>instrumentation/adf/frequencies/primary-thousands</primary-thousands>
			<primary-hundreds>instrumentation/adf/frequencies/primary-hundreds</primary-hundreds>
			<primary-tenths>instrumentation/adf/frequencies/primary-tenths</primary-tenths>
			<primary-units>instrumentation/adf/frequencies/primary-units</primary-units>
			<secondary>instrumentation/adf/frequencies/secondary-khz</secondary>
			<secondary-thousands>instrumentation/adf/frequencies/secondary-thousands</secondary-thousands>
			<secondary-hundreds>instrumentation/adf/frequencies/secondary-hundreds</secondary-hundreds>
			<secondary-tenths>instrumentation/adf/frequencies/secondary-tenths</secondary-tenths>
			<secondary-units>instrumentation/adf/frequencies/secondary-units</secondary-units>
		</frequencies>
	</params>
	
	<animation>
		<type>knob</type>
		<object-name>PriFreqMinilever1000</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property alias="../../params/frequencies/primary-thousands"/>
		<factor>-5.8</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/frequencies/primary-thousands"/>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>
					c210.adf.adjustFrequency("primary");
				</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>adf-primary-freq</tooltip-id>
				<label>ADF primary frequency: %d KHz</label>
				<measure-text>ADF primary frequency: 1000 KHz</measure-text>
				<property alias="../../../../params/frequencies/primary"/>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>PriFreqMinilever100</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property alias="../../params/frequencies/primary-hundreds"/>
		<factor>-5.8</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/frequencies/primary-hundreds"/>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>
					c210.adf.adjustFrequency("primary");
				</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>adf-primary-freq</tooltip-id>
				<label>ADF primary frequency: %d KHz</label>
				<measure-text>ADF primary frequency: 1000 KHz</measure-text>
				<property alias="../../../../params/frequencies/primary"/>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>PriFreqMinilever10</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property alias="../../params/frequencies/primary-tenths"/>
		<factor>-5.8</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/frequencies/primary-tenths"/>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>
					c210.adf.adjustFrequency("primary");
				</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>adf-primary-freq</tooltip-id>
				<label>ADF primary frequency: %d KHz</label>
				<measure-text>ADF primary frequency: 1000 KHz</measure-text>
				<property alias="../../../../params/frequencies/primary"/>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>PriFreqMinilever1</object-name>
		<axis>
			<object-name>ScaleMinileverAxis</object-name>
		</axis>
		<property alias="../../params/frequencies/primary-units"/>
		<factor>-5.8</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/frequencies/primary-units"/>
				<min>0</min>
				<max>9</max>
				<factor>1</factor>
			</binding>
			<binding>
				<command>nasal</command>
				<script>
					c210.adf.adjustFrequency("primary");
				</script>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>adf-primary-freq</tooltip-id>
				<label>ADF primary frequency: %d KHz</label>
				<measure-text>ADF primary frequency: 1000 KHz</measure-text>
				<property alias="../../../../params/frequencies/primary"/>
