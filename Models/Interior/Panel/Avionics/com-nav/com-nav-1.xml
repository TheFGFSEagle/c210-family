<?xml version="1.0"?>
<PropertyList>
	<params>
		<com-freq-mhz>instrumentation/comm[0]/frequencies/dialed-mhz</com-freq-mhz>
		<com-freq-khz>instrumentation/comm[0]/frequencies/dialed-khz</com-freq-khz>
		<com-volume>instrumentation/comm[0]/volume</com-volume>
		<com-volume-script>p210n.comNav1.adjustComVolume()</com-volume-script>
		<com-squelch>instrumentation/comm[0]/squelch</com-squelch>
		<nav-freq-mhz>instrumentation/nav[0]/frequencies/dialed-mhz</nav-freq-mhz>
		<nav-freq-khz>instrumentation/nav[0]/frequencies/dialed-khz</nav-freq-khz>
		<nav-volume>instrumentation/nav[0]/volume</nav-volume>
		<nav-mode>instrumentation/nav[0]/mode</nav-mode>
		<com-memory1-pressed>instrumentation/comm[0]/memory1-pressed</com-memory1-pressed>
		<com-memory2-pressed>instrumentation/comm[0]/memory2-pressed</com-memory2-pressed>
		<com-memory3-pressed>instrumentation/comm[0]/memory3-pressed</com-memory3-pressed>
		<nav-memory1-pressed>instrumentation/nav[0]/memory1-pressed</nav-memory1-pressed>
		<nav-memory2-pressed>instrumentation/nav[0]/memory2-pressed</nav-memory2-pressed>
		<nav-memory3-pressed>instrumentation/nav[0]/memory1-pressed</nav-memory3-pressed>
		<com-memory1-script>p210n.comNav1.memoryPressed("com", 1);</com-memory1-script>
		<com-memory2-script>p210n.comNav1.memoryPressed("com", 2);</com-memory2-script>
		<com-memory3-script>p210n.comNav1.memoryPressed("com", 3);</com-memory3-script>
		<nav-memory1-script>p210n.comNav1.memoryPressed("nav", 1);</nav-memory1-script>
		<nav-memory2-script>p210n.comNav1.memoryPressed("nav", 2);</nav-memory2-script>
		<nav-memory3-script>p210n.comNav1.memoryPressed("nav", 3);</nav-memory3-script>
		<cycle-pressed>instrumentation/comm[0]/cycle-pressed</cycle-pressed>
		<cycle-script>p210n.comNav1.cyclePressed()</cycle-script>
		<label-text>COM/NAV 1</label-text>
	</params>
	
	<path>com-nav.ac</path>
	
	<animation>
		<type>knob</type>
		<object-name>COMFreqKnobMHz</object-name>
		<axis>
			<object-name>COMFreqKnobAxis</object-name>
		</axis>
		<property alias="../../params/com-freq-mhz"/>
		<factor>20</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/com-freq-mhz"/>
				<factor>1</factor>
				<min>118</min>
				<max>135</max>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>COMFreqKnobKHz</object-name>
		<axis>
			<object-name>COMFreqKnobAxis</object-name>
		</axis>
		<property alias="../../params/com-freq-khz"/>
		<factor>200</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/com-freq-khz"/>
				<factor>0.05</factor>
				<min>0</min>
				<max>0.95</max>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>NAVFreqKnobMHz</object-name>
		<axis>
			<object-name>NAVFreqKnobAxis</object-name>
		</axis>
		<property alias="../../params/nav-freq-mhz"/>
		<factor>20</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/nav-freq-mhz"/>
				<factor>1</factor>
				<min>108</min>
				<max>117</max>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>NAVFreqKnobKHz</object-name>
		<axis>
			<object-name>NAVFreqKnobAxis</object-name>
		</axis>
		<property alias="../../params/nav-freq-khz"/>
		<factor>200</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/nav-freq-khz"/>
				<factor>0.05</factor>
				<min>0</min>
				<max>0.95</max>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>COMVolumeKnob</object-name>
		<axis>
			<object-name>COMVolumeKnobAxis</object-name>
		</axis>
		<property alias="../../params/com-volume"/>
		<factor>180</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/com-volume"/>
				<factor>0.05</factor>
				<min>0</min>
				<max>1</max>
			</binding>
			<binding>
				<command>nasal</command>
				<script alias="../../../../params/com-volume-script"/>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>NAVVolumeKnob</object-name>
		<axis>
			<object-name>NAVVolumeKnobAxis</object-name>
		</axis>
		<property alias="../../params/nav-volume"/>
		<factor>180</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/nav-volume"/>
				<factor>0.05</factor>
				<min>0</min>
				<max>1</max>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>COMSquelchKnob</object-name>
		<axis>
			<object-name>COMSquelchKnobAxis</object-name>
		</axis>
		<property alias="../../params/com-squelch"/>
		<factor>180</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/com-squelch"/>
				<factor>0.05</factor>
				<min>0</min>
				<max>1</max>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>NAVModeSwitch</object-name>
		<axis>
			<object-name>NAVModeSwitchAxis</object-name>
		</axis>
		<property alias="../../params/nav-mode"/>
		<factor>-20</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property alias="../../../../params/nav-mode"/>
				<factor>1</factor>
				<min>-1</min>
				<max>1</max>
			</binding>
		</action>
		<release>
			<binding>
				<condition>
					<equals>
						<property alias="../../../../../params/nav-mode"/>
						<value>-1</value>
					</equals>
				</condition>						
				<command>property-adjust</command>
				<property alias="../../../../params/nav-mode"/>
				<factor>0</factor>
				<min>0</min>
				<max>1</max>
			</binding>
		</release>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>COMMemory1Button</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property alias="../../params/com-memory1-pressed"/>
		<factor>0.003</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property alias="../../../../params/com-memory1-pressed"/>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script alias="../../../../params/com-memory1-script"/>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property alias="../../../../params/com-memory1-pressed"/>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	<animation>
		<type>slider</type>
		<object-name>COMMemory2Button</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property alias="../../params/com-memory2-pressed"/>
		<factor>0.003</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property alias="../../../../params/com-memory2-pressed"/>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script alias="../../../../params/com-memory2-script"/>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property alias="../../../../params/com-memory2-pressed"/>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	<animation>
		<type>slider</type>
		<object-name>COMMemory3Button</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property alias="../../params/com-memory3-pressed"/>
		<factor>0.003</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property alias="../../../../params/com-memory3-pressed"/>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script alias="../../../../params/com-memory3-script"/>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property  alias="../../../../params/com-memory3-pressed"/>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	<animation>
		<type>slider</type>
		<object-name>NAVMemory1Button</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property alias="../../params/nav-memory1-pressed"/>
		<factor>0.003</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property alias="../../../../params/nav-memory1-pressed"/>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script alias="../../../../params/nav-memory1-script"/>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property alias="../../../../params/nav-memory1-pressed"/>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	<animation>
		<type>slider</type>
		<object-name>NAVMemory2Button</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property alias="../../params/nav-memory2-pressed"/>
		<factor>0.003</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property alias="../../../../params/nav-memory2-pressed"/>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script alias="../../../../params/nav-memory2-script"/>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property alias="../../../../params/nav-memory2-pressed"/>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	<animation>
		<type>slider</type>
		<object-name>NAVMemory3Button</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property alias="../../params/nav-memory3-pressed"/>
		<factor>0.003</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property alias="../../../../params/nav-memory3-pressed"/>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script alias="../../../../params/nav-memory3-script"/>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property alias="../../../../params/nav-memory3-pressed"/>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>CycleButton</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property alias="../../params/cycle-pressed"/>
		<factor>0.003</factor>
		<action>
			<binding>
				<command>property-assign</command>
				<property alias="../../../../params/cycle-pressed"/>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script alias="../../../../params/cycle-script"/>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-interpolate</command>
				<property alias="../../../../params/cycle-pressed"/>
				<value>0</value>
				<time>0.01</time>
			</binding>
		</release>
	</animation>
	
	<text>
		<name>Label</name>
		<type type="string">literal</type>
		<text type="string" alias="../../params/label-text"/>
		<draw-text type="bool">true</draw-text>
		<font>Helvetica.txf</font>
		<character-size type="double">0.015</character-size>
		<character-aspect-ratio type="double">1.0</character-aspect-ratio>
		<axis-alignment>yz-plane</axis-alignment>
		<max-height>0.08</max-height>
		<max-width>0.08</max-width>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
		<alignment>center-center</alignment>
		<offsets>
			<x-m>0.01</x-m>
			<y-m>0.0 </y-m>
			<z-m>0.02</z-m>
		</offsets>
	</text>
	
	<animation>
		<type>material</type>
		<object-name>Label</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Label</object-name>
		<condition>
			<property>/sim/panel-hotspots</property>
		</condition>
	</animation>
	
	<effect>
		<inherits-from>Effects/glass</inherits-from>
		<object-name>Glass</object-name>
	</effect>
</PropertyList>
