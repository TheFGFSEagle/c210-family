<?xml version="1.0" encoding="UTF-8"?>

<PropertyList include="base.xml">
	<path>silver-eagle.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Fuselage</object-name>
		<object-name>Rudder</object-name>
		<object-name>NavAntennas</object-name>
		<object-name>LeftElevatorMount</object-name>
		<object-name>LeftElevator</object-name>
		<object-name>RightElevatorMount</object-name>
		<object-name>RightElevator</object-name>
		<object-name>ElevatorTrimTab</object-name>
		<object-name>RudderMount</object-name>
		<object-name>Rudder</object-name>
		<object-name>LeftForwardNoseGearDoorExterior</object-name>
		<object-name>LeftAftNoseGearDoorExterior</object-name>
		<object-name>RightForwardNoseGearDoorExterior</object-name>
		<object-name>RightAftNoseGearDoorExterior</object-name>
		<object-name>LeftCowlFlapExterior</object-name>
		<object-name>RightCowlFlapExterior</object-name>
		<object-name>ELTAntenna</object-name>
		<object-name>TransponderAntenna</object-name>
		<object-name>MarkerBeaconAntenna</object-name>
		<object-name>WeatherRadarAntenna</object-name>
		<object-name>PilotDoorExterior</object-name>
		<object-name>EmergencyExitExterior</object-name>
		<object-name>BaggageDoorExterior</object-name>
		<property-base>sim/model/livery</property-base>
		<texture-prop>texture-fuselage</texture-prop>
		<texture>fuselage.png</texture>
	</animation>
	<effect>
		<inherits-from>Effects/p210n-interior-glass</inherits-from>
		<object-name>WindscreenInterior</object-name>
		<object-name>PilotWindowsInterior</object-name>
		<object-name>EmergencyExitWindowsInterior</object-name>
		<object-name>LeftPassengerWindowsInterior</object-name>
		<object-name>RightPassengerWindowsInterior</object-name>
		<object-name>BackWindowsInterior</object-name>
	</effect>
	<effect>
		<inherits-from>Effects/p210n-interior</inherits-from>
		<object-name>Interior</object-name>
		<object-name>PilotDoorInterior</object-name>
		<object-name>EmergencyExitInterior</object-name>
		<object-name>BaggageDoorInterior</object-name>
		<object-name>BaggageCompartment</object-name>
	</effect>
	
	<nasal>
		<load>
			<![CDATA[
				# called only when the model is loaded as an AI model (multiplayer)
				var livery_update = aircraft.livery_update.new("Aircraft/c210-family/Models/Liveries/silver-eagle");
			]]>
		</load>
		<unload>
			<![CDATA[
				livery_update.stop();
			]]>
		</unload>
	</nasal>
	
	<animation>
		<type>rotate</type>
		<object-name>LeftForwardNoseGearDoorExterior</object-name>
		<object-name>LeftForwardNoseGearDoorInterior</object-name>
		<axis>
			<object-name>LeftForwardNoseGearDoorAxis</object-name>
		</axis>
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>30</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>70</dep>
			</entry>
			<entry>
				<ind>0.3</ind>
				<dep>70</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>LeftAftNoseGearDoorExterior</object-name>
