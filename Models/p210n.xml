		<object-name>PropellerBlade3Damaged</object-name>
		<condition>
			<and>
				<property>/fdm/jsbsim/damage/propeller</property>
				<less-than>
					<property>engines/engine/rpm</property>
					<value>1000</value>
				</less-than>
