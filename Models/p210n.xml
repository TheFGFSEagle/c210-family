		<action>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					c210.pilotDoor.toggle();
				</script>
			</binding>
		</action>
		<shift-increase>
			<binding>
				<command>nasal</command>
				<script><![CDATA[
					c210.pilotDoor.move(c210.pilotDoor.getpos() + 0.05);
				]]></script>
			</binding>
		</shift-increase>
		<shift-decrease>
