<?xml version="1.0"?>
<system name="engine">
    <property>propulsion/engine/auto-mixture</property>
    <property>propulsion/engine/AFR-setting</property>
    <channel name="Volumetric efficiency">
        <fcs_function>
            <function>
                <table>
                    <independentVar>atmosphere/P-psf</independentVar>
                    <tableData>
                        1006.086   0.6067
                        1014.51     0.6077
                        1057.253   0.6181
                        1101.501   0.6233
                        1147.261   0.6303
                        1194.568   0.6365
                        1243.578   0.6429
                        1293.893   0.6491
                        1345.994   0.6555
                        1399.785   0.6621
                        1455.481   0.6685
                        1512.65     0.6753
                        1571.9       0.6821
                        1632.86     0.6891
                        1695.72     0.6959
                        1760.594   0.7031
                        1827.434   0.7101
                        1896.355   0.7343
                        1967.372   0.7697
                        2040.768   0.8065
                        2115.885   0.8447
                    </tableData>
                </table>
            </function>
            <output>propulsion/engine[0]/volumetric-efficiency</output>
        </fcs_function>
    </channel>

    <channel name="Auto mixture" execute="propulsion/engine[0]/auto-mixture">
        <switch name="propulsion/engine[0]/mixture-adjust">
            <default value="0"/>
                <test logic="AND" value="-0.0001">
                propulsion/engine[0]/AFR LT propulsion/engine[0]/AFR-setting
                propulsion/engine[0]/set-running EQ 1
            </test>
            <test logic="AND" value="0.0001">
                propulsion/engine[0]/AFR GT propulsion/engine[0]/AFR-setting
                propulsion/engine[0]/set-running EQ 1
            </test>
        </switch>
        <summer>
            <input>/controls/engines/engine[0]/mixture</input>
            <input>propulsion/engine[0]/mixture-adjust</input>
            <output>/controls/engines/engine[0]/mixture</output>
            <clipto>
                <min>0</min>
                <max>1</max>
            </clipto>
        </summer>
    </channel>

    <channel name="BSFC">
        <fcs_function>
            <function>
                <table>
                    <independentVar>atmosphere/P-psf</independentVar>
                        <tableData>
                           1006.086   0.4467
                           1014.51      0.4465
                           1057.253   0.4475
                           1101.501   0.4473
                           1147.261   0.4477
                           1194.568   0.4475
                           1243.578   0.4475
                           1293.893   0.4477
                           1345.994   0.4475
                           1399.785   0.4475
                           1455.481   0.4475
                           1512.65     0.4477
                           1571.9       0.4479
                           1632.86     0.4479
                           1695.72     0.4479
                           1760.594   0.4479
                           1827.434   0.4479
                           1896.355   0.4549
                           1967.372   0.4653
                           2040.768   0.4745
                           2115.885   0.4811
                        </tableData>
                    </table>
                </function>
            <output>propulsion/engine[0]/bsfc-lbs_hphr</output>
        </fcs_function>
    </channel>
    
    <!-- Allow disabling of engine time recording for testing purposes -->
    <property value="1">propulsion/engine/record-tach-time</property>
    <channel name="Engine tach time" execrate="120" execute="propulsion/engine/record-tach-time">
        <fcs_function>
            <function>
                <sum>
                    <quotient>
                        <property>propulsion/engine/engine-rpm</property>
                        <value>2500</value>
                    </quotient>
                    <property>propulsion/engine/tach-time-sec</property>
                </sum>
            </function>
            <output>propulsion/engine/tach-time-sec</output>
        </fcs_function>
        
        <fcs_function>
            <function>
                <quotient>
                    <property>propulsion/engine/tach-time-sec</property>
                    <value>3600</value>
                </quotient>
            </function>
            <output>propulsion/engine/tach-time-hours</output>
        </fcs_function>
    </channel>
</system>
