<?xml version="1.0"?>
<PropertyList include="init-instrumentation-base.xml">
	<engine>
		<indicated-egt-degf alias="/engines/engine/egt-degf"/>
		<indicated-mp-inhg alias="/engines/engine/mp-inhg"/>
		<indicated-fuelflow-pph alias="/engines/engine/fuel-flow_pph"/>
		<indicated-engine-rpm alias="/engines/engine/rpm"/>
		<indicated-tach-time-hours alias="/fdm/jsbsim/propulsion/engine/tach-time-hours"/>
		<indicated-cht-degf alias="/engines/engine/cht-degf"/>
		<indicated-oil-temperature-degf alias="/engines/engine/oil-temperature-degf"/>
		<indicated-oil-pressure-psi alias="/engines/engine/oil-pressure-psi"/>
		<indicated-battery-charge-rate-amps alias="/systems/electrical/ammeter-needle-amps"/>
	</engine>
	
	<cabin-altimeter>
		<serviceable type="bool">true</serviceable>
		<setting-inhg type="float">29.92</setting-inhg>
	</cabin-altimeter>
	
	<cabin-vertical-speed-indicator>
		<serviceable type="bool">true</serviceable>
	</cabin-vertical-speed-indicator>
</PropertyList>
