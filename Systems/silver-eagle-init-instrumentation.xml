<?xml version="1.0"?>
<PropertyList include="init-instrumentation-base.xml">
	<!-- most of the stuff before <altimeter n="1"> should be replaced by system files to support electrcial / other system failures -->
	<engine>
		<indicated-amps alias="/systems/electrical/amps"/>
		<indicated-volts alias="/systems/electrical/volts"/>
		<!-- TODO: implement fuel pressure calculation -->
		<indicated-fuel-pressure-psi alias="/engines/engine/fuel-pressure-psi"/>
		<indicated-n1-percent alias="/engines/engine/n1"/>
		<!-- TODO: implement TOT calculation -->
		<indicated-tot-degc alias="/engines/engine/tot-degc"/>
		<indicated-torque-psi alias="/engines/engine/thruster/torque-psi"/>
		<!-- TODO: convert Â°F to Â°C -->
		<indicated-oil-temperature-degc alias="/engines/engine/oil-temperature-degf"/>
		<indicated-oil-pressure-psi alias="/engines/engine/oil-pressure-psi"/>
		<indicated-propeller-rpm alias="/engines/engine/thruster/rpm"/>
	</engine>
	
	<cabin-altimeter>
		<serviceable type="bool">true</serviceable>
	</cabin-altimeter>
	
	<cabin-vertical-speed-indicator>
		<serviceable type="bool">true</serviceable>
	</cabin-vertical-speed-indicator>
</PropertyList>
