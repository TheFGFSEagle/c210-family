<?xml version="1.0"?>
<PropertyList>
	<electrical>
		<name>electrical</name>
		<number>0</number>
		<path>Systems/electrical.xml</path>
	</electrical>
	
	<pitot>
		<name>pitot</name>
		<number>0</number>
		<stall-deg>60</stall-deg>
	</pitot>
	
	<static n="0"> <!-- Primary source is from fuselage, see POH p. 7-42 -->
			<name>static</name>
			<number>0</number>
			<tau>0.1</tau>
			<type>1</type>
			<error-factor>0.2</error-factor>
	</static>
	<static n="1"> <!-- Alternate source is from baggage area, see POH p. 7-42 -->
			<name>static</name>
			<number>1</number>
			<tau>0.1</tau>
			<type>0</type>
			<error-factor>0.35</error-factor>
	</static>
	
	<vacuum>
		<name>vacuum</name>
		<number>0</number>
		<rpm>/engines/engine[0]/rpm</rpm>
		<scale>0.04</scale>
	</vacuum>
</PropertyList>

